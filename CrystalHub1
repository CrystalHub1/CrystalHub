local function WYABMB_fake_script() -- CreditsPageB.LocalScript 
	local script = Instance.new('LocalScript', CreditsPageB)

	script.Parent.MouseEnter:Connect(function()
		game:GetService("TweenService"):Create(script.Parent, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(121, 157, 255)}):Play()
	end)
	script.Parent.MouseLeave:Connect(function()
		game:GetService("TweenService"):Create(script.Parent, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(47, 47, 47)}):Play()
	end)
end
